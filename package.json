    "./vector": {
			"require": "./dist/vector.js",
			"types": "./dist/vector.d.ts"
		}
  },
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JonnyJong/abm-utils.git"
	},
	"author": "jonnyjonny",

			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
    "./array": {
			"require": "./dist/array.js",
			"types": "./dist/array.d.ts"
		},
    "./color": {
			"require": "./dist/color.js",
			"types": "./dist/color.d.ts"
		},
    "./function": {
			"require": "./dist/function.js",
			"types": "./dist/function.d.ts"
		},

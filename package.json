		"test": "jest",
		"test-coverage": "jest --coverage",
		"lint": "biome check"
	},
	"devDependencies": {
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"@types/jest": "^29.5.13",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.5",
		"@biomejs/biome": "1.9.4"
	}
